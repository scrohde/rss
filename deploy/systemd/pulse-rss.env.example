# Copy to /etc/pulse-rss/pulse-rss.env and set secure values.
PORT=8080
LOG_LEVEL=info
DB_PATH=/var/lib/pulse-rss/rss.db

AUTH_ENABLED=true
AUTH_RP_ID=rss.example.com
AUTH_RP_ORIGIN=https://rss.example.com
AUTH_RP_NAME=Pulse RSS
AUTH_SETUP_TOKEN=replace-with-long-random-secret
AUTH_SESSION_TTL=24h
AUTH_CHALLENGE_TTL=5m
AUTH_COOKIE_NAME=pulse_rss_session
AUTH_COOKIE_SECURE=true
